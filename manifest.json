{
    "manifest_version": 3,
    "name": "NoteTide_Y",
    "version": "1.0",
    "permissions": [
      "alarms",
      "notifications",
      "storage",
      "activeTab"
    ],

    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/Notetide16_y.png", 
        "48": "images/Notetide48_y.png",
        "128": "images/Notetide128_y.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  